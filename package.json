{
  "name": "left-phalange",
  "version": "1.1.2",
  "description": "Convert data between CommonJS, ESModule, INI, JSON, JSON5, TOML, YAML",
  "main": "lib/index.js",
  "repository": "fisker/left-phalange",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "keywords": [
    "data",
    "convert",
    "json",
    "yaml",
    "json5",
    "toml",
    "ini"
  ],
  "license": "MIT",
  "files": [
    "bin"
  ],
  "bin": {
    "left-phalange": "bin/cli",
    "lp": "bin/cli"
  },
  "dependencies": {
    "ansi-colors": "4.1.0",
    "clipboardy": "2.1.0",
    "get-stdin": "7.0.0",
    "left-phalange-api": "1.1.4",
    "meow": "5.0.0",
    "update-notifier": "3.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "scripts": {
    "release": "run-s lint build format test dist",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint **/*.md --ignore \"**/node_modules/**\"",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "test": "ava --verbose",
    "test-coverage": "nyc yarn test",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "sideEffects": false,
  "devDependencies": {
    "@commitlint/cli": "8.0.0",
    "@fisker/commitlint-config": "1.1.0",
    "@fisker/eslint-config": "1.2.4",
    "@fisker/eslint-config-ava": "1.2.0",
    "@fisker/husky-config": "1.1.0",
    "@fisker/lint-staged-config": "1.0.4",
    "@fisker/prettier-config": "1.0.17",
    "ava": "2.1.0",
    "cz-conventional-changelog-emoji": "0.1.0",
    "eslint": "6.0.1",
    "esm": "3.2.25",
    "execa": "2.0.2",
    "husky": "3.0.0",
    "lint-staged": "9.0.0",
    "markdownlint-cli": "0.17.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "rollup": "1.16.4",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-filesize": "6.1.1",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0"
  },
  "ava": {
    "require": "esm",
    "verbose": true,
    "helpers": [
      "test/helpers/**/*"
    ]
  }
}
