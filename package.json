{
  "name": "left-phalange",
  "version": "1.1.4",
  "description": "Convert data between CommonJS, ESModule, INI, JSON, JSON5, TOML, YAML",
  "main": "lib/index.js",
  "repository": "fisker/left-phalange",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "keywords": [
    "data",
    "convert",
    "json",
    "yaml",
    "json5",
    "toml",
    "ini"
  ],
  "license": "MIT",
  "files": [
    "bin"
  ],
  "bin": {
    "left-phalange": "bin/cli",
    "lp": "bin/cli"
  },
  "dependencies": {
    "ansi-colors": "4.1.1",
    "clipboardy": "2.1.0",
    "get-stdin": "7.0.0",
    "left-phalange-api": "1.1.8",
    "meow": "5.0.0",
    "update-notifier": "3.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint build format test dist",
    "test": "ava",
    "test-coverage": "c8 ava"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "sideEffects": false,
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@fisker/commitlint-config": "1.1.3",
    "@fisker/eslint-config": "1.3.4",
    "@fisker/eslint-config-ava": "1.2.4",
    "@fisker/husky-config": "1.1.3",
    "@fisker/lint-staged-config": "1.0.6",
    "@fisker/prettier-config": "1.0.21",
    "ava": "2.4.0",
    "c8": "6.0.1",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.6.0",
    "esm": "3.2.25",
    "execa": "3.2.0",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "markdownlint-cli": "0.19.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "rollup": "1.26.2",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0"
  },
  "ava": {
    "require": "esm",
    "verbose": true,
    "helpers": [
      "test/helpers/**/*"
    ]
  }
}
