{
  "name": "left-phalange",
  "version": "1.1.0",
  "description": "Convert data between CommonJS, ESModule, INI, JSON, JSON5, TOML, YAML",
  "main": "lib/index.js",
  "repository": "fisker/left-phalange",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "keywords": [
    "data",
    "convert",
    "json",
    "yaml",
    "json5",
    "toml",
    "ini"
  ],
  "license": "MIT",
  "files": [
    "bin"
  ],
  "bin": {
    "left-phalange": "bin/cli",
    "lp": "bin/cli"
  },
  "dependencies": {
    "ansi-colors": "^3.2.4",
    "clipboardy": "^2.0.0",
    "get-stdin": "^7.0.0",
    "left-phalange-api": "^1.1.4",
    "meow": "^5.0.0",
    "update-notifier": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "scripts": {
    "release": "run-s lint build prettier test dist",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint **/*.{js,mjs,vue} --fix",
    "lint:markdown": "markdownlint **/*.md --ignore \"**/node_modules/**\"",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "prettier": "prettier **/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml} --write",
    "test": "ava --verbose",
    "test-coverage": "nyc yarn test"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "sideEffects": false,
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@fisker/commitlint-config": "^1.0.9",
    "@fisker/eslint-config": "^1.1.0",
    "@fisker/eslint-config-ava": "^1.1.0",
    "@fisker/husky-config": "^1.1.0",
    "@fisker/lint-staged-config": "^1.0.4",
    "@fisker/prettier-config": "^1.0.17",
    "ava": "^2.1.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^5.16.0",
    "esm": "^3.2.25",
    "execa": "^1.0.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.0",
    "markdownlint-cli": "^0.16.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rollup": "^1.15.1",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-filesize": "^6.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.1"
  },
  "ava": {
    "require": "esm",
    "verbose": true,
    "helpers": [
      "test/helpers/**/*"
    ]
  }
}
