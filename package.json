{
  "name": "left-phalange",
  "version": "1.1.5",
  "description": "Convert data between CommonJS, ESModule, INI, JSON, JSON5, TOML, YAML",
  "keywords": [
    "convert",
    "data",
    "ini",
    "json",
    "json5",
    "toml",
    "yaml"
  ],
  "repository": "fisker/left-phalange",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "dist/cli",
  "bin": {
    "left-phalange": "dist/cli",
    "lp": "dist/cli"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "clean:js": "del-cli dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint build format test dist",
    "test": "ava",
    "test-coverage": "c8 ava"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "helpers": [
      "test/helpers/**/*"
    ],
    "require": "esm",
    "verbose": true
  },
  "dependencies": {
    "ansi-colors": "4.1.1",
    "clipboardy": "2.1.0",
    "get-stdin": "7.0.0",
    "left-phalange-api": "1.1.10",
    "meow": "6.0.0",
    "update-notifier": "3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@fisker/commitlint-config": "1.1.4",
    "@fisker/eslint-config": "1.5.1",
    "@fisker/eslint-config-ava": "1.2.5",
    "@fisker/husky-config": "1.1.5",
    "@fisker/lint-staged-config": "1.0.8",
    "@fisker/prettier-config": "1.0.24",
    "@rollup/plugin-json": "4.0.0",
    "@rollup/plugin-node-resolve": "6.0.0",
    "ava": "2.4.0",
    "c8": "6.0.1",
    "cz-conventional-changelog": "3.0.2",
    "del-cli": "3.0.0",
    "eslint": "6.7.2",
    "esm": "3.2.25",
    "execa": "3.4.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "markdownlint-cli": "0.19.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "rollup": "1.27.9",
    "rollup-plugin-commonjs": "10.1.0",
    "sort-package-json": "1.30.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
