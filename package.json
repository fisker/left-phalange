{
  "name": "left-phalange",
  "version": "2.0.0",
  "description": "Convert data between CommonJS, ESModule, INI, JSON, JSON5, TOML, YAML",
  "keywords": [
    "convert",
    "data",
    "ini",
    "json",
    "json5",
    "toml",
    "yaml"
  ],
  "homepage": "https://github.com/fisker/left-phalange#readme",
  "bugs": {
    "url": "https://github.com/fisker/left-phalange/issues"
  },
  "repository": "fisker/left-phalange",
  "funding": "https://github.com/fisker/left-phalange?sponsor=1",
  "license": "MIT",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "main": "dist/cli",
  "bin": {
    "left-phalange": "dist/cli",
    "lp": "dist/cli"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "run-p clean:*",
    "clean:dist": "del-cli dist",
    "clean:js": "del-cli dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,cjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,cjs,mjs,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint build format test dist",
    "test": "ava",
    "test-coverage": "c8 yarn test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "verbose": true
  },
  "dependencies": {
    "ansi-colors": "4.1.1",
    "clipboardy": "2.3.0",
    "get-stdin": "8.0.0",
    "import-commonjs": "0.0.3",
    "left-phalange-api": "1.3.3",
    "meow": "8.1.2",
    "update-notifier": "5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@fisker/commitlint-config": "1.3.1",
    "@fisker/eslint-config": "4.1.1",
    "@fisker/eslint-config-ava": "2.0.1",
    "@fisker/husky-config": "4.0.1",
    "@fisker/lint-staged-config": "3.0.1",
    "@fisker/prettier-config": "4.0.1",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "ava": "3.15.0",
    "c8": "7.5.0",
    "cz-conventional-changelog": "3.3.0",
    "del-cli": "3.0.1",
    "eslint": "7.20.0",
    "execa": "5.0.0",
    "husky": "5.0.9",
    "lint-staged": "10.5.4",
    "markdownlint-cli": "0.26.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "rollup": "2.39.0",
    "sort-package-json": "1.48.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
